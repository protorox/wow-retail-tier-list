# Core
NODE_ENV=development
PORT=3000
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/wow_tier_list?schema=public
REDIS_URL=redis://localhost:6379

# Data source settings
MOCK_MODE=false
RAIDER_IO_BASE_URL=https://raider.io
RAIDER_IO_MPLUS_ENDPOINT=/api/v1/mythic-plus/runs
WARCRAFTLOGS_BASE_URL=https://www.warcraftlogs.com
WARCRAFTLOGS_CLIENT_ID=
WARCRAFTLOGS_CLIENT_SECRET=
WARCRAFTLOGS_ZONE_ID=38
WARCRAFTLOGS_DIFFICULTY=5
WARCRAFTLOGS_MPLUS_ZONE_ID=20
WARCRAFTLOGS_MPLUS_DIFFICULTY=10
WARCRAFTLOGS_MPLUS_BRACKET=10
WARCRAFTLOGS_MPLUS_PAGES=12

# Scheduling
REFRESH_INTERVAL_MINUTES=30
WORKER_MODE=interval
QUEUE_NAME=wow-tier-refresh
QUEUE_CONCURRENCY=2
API_CONCURRENCY=4
CRON_SECRET=replace-me

# Admin auth
ADMIN_USERNAME=admin
ADMIN_PASSWORD=admin

# Feature flags
ENABLE_SEED_ON_START=true
